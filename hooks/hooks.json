{
  "hooks": {
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "agentdb read-start 2>/dev/null || true",
            "timeout": 5
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "agentdb write-end '{\"session\":\"ended\"}' 2>/dev/null || true",
            "timeout": 5
          }
        ]
      }
    ]
  }
}
